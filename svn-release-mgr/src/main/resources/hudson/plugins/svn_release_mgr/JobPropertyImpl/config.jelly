<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <f:optionalBlock name="releases.show"
        help="plugin/svn-release-mgr/help-projectConfig.html"
        title="${%Use Subversion Release Manager}" checked="${instance != null}">
    <f:entry title="${%Max Revisions to show}">
      <f:textbox field="maxRevisions" name="maxRevisions" clazz="required positive-number"/>
    </f:entry>
  </f:optionalBlock>
</j:jelly>
