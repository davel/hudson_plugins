            <exclude>**/AuthzTest*</exclude>
            <exclude>**/ProjectAuthTest*</exclude>
            <exclude>**/DocUploadTest*</exclude>
            <exclude>**/FileReleaseTest*</exclude>
            <exclude>**/PblUploadTest*</exclude>
            <exclude>**/TrackerTest*</exclude>
          </excludes>
